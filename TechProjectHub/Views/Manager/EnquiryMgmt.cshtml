
@{
    ViewBag.Title = "EnquiryMgmt";
    Layout = "~/Views/Shared/Common_Manager_Layout.cshtml";
}
@using TechProjectHub.Models
@model IEnumerable<EnquiryMaster> 

<div class="row">
    <div class="col-sm-1"></div>
    <div class="col-sm-12 pt-5 pb-5 mt-5 mb-5 bg-light" style="border-radius:15px">
        <h3 class="text-center text-primary mt-1">Request from Developers</h3>
        <table class="table text-center table-striped">
            <tr>
                <th>Enquiry Id</th>
                <th>Name</th>
                <th>Email Id</th>
                <th>Mobile No</th>
                <th>Enquiry Query</th>
                <th>Enquery Date</th>
            </tr>
            @{

                foreach (EnquiryMaster em in Model)
                {

                    <tr>
                        <td>@em.Enq_Id</td>
                        <td>@em.Name</td>
                        <td>@em.EmailId</td>
                        <td>@em.MobNo</td>
                        <td>@em.Query</td>
                        <td>@em.Enq_DT</td>
                    </tr>
                }

            }
        </table>
    </div>
    <div class="col-sm-1"></div>
</div>

